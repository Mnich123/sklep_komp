package Logika;


public class Sklep implements Runnable {

	private String nazwa;

	public Sklep(String nazwa) {
		this.nazwa = nazwa;
		Dane.Semafory.semSklep.release();
	}

	public Sklep() {

	}

	
	public Sklep pobierzSklep() {
		return this;
	}
	
	public String pobierzNazwe(){
		return this.nazwa;
	}

	public void run() {
		while (true) {

		}
	}

}
