package Logika;

public class Produkt {

	private int id;
	private String nazwa;
	private int cena;

	public Produkt(int id, String nazwa, int cena) {
		this.id = id;
		this.nazwa = nazwa;
		this.cena = cena;
	}

	public int pobierzCene() {
		return this.cena;
	}

	public String pobierzNazwe() {
		return this.nazwa;
	}

	public int pobierzId() {
		return this.id;
	}
	
	@Override
	public int hashCode(){
		return id;
	}
	public boolean equels(Object o){
		Produkt p = (Produkt) o ;
		
		return p.pobierzId() == ((Produkt) o).pobierzId() ;
	}

}
